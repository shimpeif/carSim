<?xml version="1.0" encoding="ISO-8859-1" standalone="no"?>
<!DOCTYPE product PUBLIC "-//Tricklab//DTD Product V1.0//EN" "Product.dtd">
<product background_color="#ede9e3" foreground_color="#000000" version="1.0">
    <title>Page</title>
    <tstart>-1.0E20</tstart>
    <tstop>1.0E20</tstop>
    <frequency>0.0</frequency>
    <page background_color="#ede9e3" foreground_color="#000000" hcells="0" vcells="0">
        <title>Ball X,Y Position, Velocity and Acceleration States</title>
        <plot background_color="#ede9e3" foreground_color="#000000" grid="Yes" grid_color="#ffffff">
            <title>Plot Position</title>
            <xaxis>
                <label>time</label>
            </xaxis>
            <yaxis>
                <label>Position</label>
            </yaxis>
            <curve>
                <var label="time" units="s">sys.exec.out.time</var>
                <var gnuplot_line_style="lines" label="position" line_color="#0000ff" line_style="Plain" symbol_size="Tiny" symbol_style="None" units="m">ball.state.output.position[0]</var>
            </curve>
            <curve>
                <var label="time" units="s">sys.exec.out.time</var>
                <var gnuplot_line_style="lines" label="index" line_color="#ff0000" line_style="Med_Fine_Dash" symbol_size="Tiny" symbol_style="None" units="m">ball.state.output.position[1]</var>
            </curve>
        </plot>
    </page>
</product>
